{
  "author": "Lam Nguyen",
  "name": "utility-kits",
  "description": "A comprehensive collection of basic utility functions and component for React ( React >= 18)",
  "version": "1.0.79",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "keywords": [
    "utility-kit",
    "utility-kits",
    "npm",
    "typescript",
    "javascript",
    "isEmpty",
    "isNull",
    "typescript",
    "utils",
    "loukas"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/thuyetlamm/utility-kits.git"
  },
  "homepage": "https://github.com/thuyetlamm/utility-kits.git",
  "scripts": {
    "build": "tsup ./src",
    "test": "yarn build && jest",
    "lint": "eslint 'src/**/*.{js,ts,tsx,jsx}'",
    "format": "prettier --write 'src/**/*.{js,ts,json,css,md}'",
    "prepare": "yarn build && husky install"
  },
  "lint-staged": {
    "*.{js, jsx,ts,tsx}": [
      "eslint --quiet --fix"
    ],
    "*.{json,js,ts,jsx,tsx,html}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@eslint/js": "8.56.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "babel-jest": "^29.7.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "globals": "^15.8.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-config": "^29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.1.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0"
  },
  "license": "MIT",
  "dependencies": {
    "react": "^18.3.1"
  }
}
